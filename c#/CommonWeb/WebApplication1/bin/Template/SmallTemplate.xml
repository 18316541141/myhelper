      "BaseRequest":{
        "Uin":$entity.Uin$,
        "Sid":"$entity.Sid$",
        "Skey":"$entity.Skey$",
        "DeviceID":"$entity.DeviceID$"
      },
      "Msg":{
        "Type":1,
        "Content":"$entity.Content$",
        "FromUserName":"$entity.FromUserName$",
        "ToUserName":"$entity.ToUserName$",
        "LocalID":"$entity.LocalID$",
        "ClientMsgId":"$entity.ClientMsgId$"
      },
      "Scene":0
    }]]>
  </webwxsendmsg>
  <webwxbatchgetcontact>
    <![CDATA[{
      "BaseRequest": {
        "Uin":$entity.Uin$,
        "Sid":"$entity.Sid$",
        "Skey":"$entity.Skey$",
        "DeviceID":"$entity.DeviceID$"
      },
      "Count":$entity.Count$,
      "List":[
        $entity.EncryChatRoomList:{
          {"UserName":"$it.UserName$","EncryChatRoomId":""}
        };separator=","$
        $entity.ChatRoomList:{
          {"UserName":"$it.UserName$","ChatRoomId":""}
        };separator=","$
      ]
    }]]>
