        }
    }]]>
  </webwxinit>
  <webwxstatusnotify>
    <![CDATA[{
      "BaseRequest": {
        "Uin":$entity.Uin$,
        "Sid":"$entity.Sid$",
        "Skey":"$entity.Skey$",
        "DeviceID":"$entity.DeviceID$"
      },
      "Code":3,
      "FromUserName":"$entity.UserName$",
      "ToUserName":"$entity.UserName$",
      "ClientMsgId":$entity.ClientMsgId$
