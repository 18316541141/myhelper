  <webwxsync>
    <![CDATA[{
        "BaseRequest":{
            "Uin":$entity.Uin$,
            "Sid":"$entity.Sid$",
            "Skey":"$entity.Skey$",
            "DeviceID":"$entity.DeviceID$"
        },
        "SyncKey":{
            "Count":$entity.Count$,
            "List":[$entity.List:{{"Key":$it.Key$,"Val":$it.Val$}};separator=","$]
        },
        "rr":$entity.RandomNum$
    }]]>
